{
  "family": "aura-vote-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::346239809653:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "aura-vote-backend",
      "image": "PLACEHOLDER_IMAGE",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/aura-vote-backend",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },

      "secrets": [
        {
          "name": "NEON_DB_POSTGRES_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:346239809653:secret:aura-vote/secrets:NEON_DB_POSTGRES_URL::"
        },
        {
          "name": "FIREBASE_PROJECT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:346239809653:secret:aura-vote/secrets:FIREBASE_PROJECT_ID::"
        },
        {
          "name": "FIREBASE_CLIENT_EMAIL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:346239809653:secret:aura-vote/secrets:FIREBASE_CLIENT_EMAIL::"
        },
        {
          "name": "FIREBASE_PRIVATE_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:346239809653:secret:aura-vote/secrets:FIREBASE_PRIVATE_KEY::"
        },
        {
          "name": "S3_BUCKET_NAME",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:346239809653:secret:aura-vote/secrets:S3_BUCKET_NAME::"
        },
        {
          "name": "NODE_ENV",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:346239809653:secret:aura-vote/secrets:NODE_ENV::"
        },
        {
          "name": "PADDLE_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:346239809653:secret:aura-vote/secrets:PADDLE_API_KEY::"
        },
        {
          "name": "PADDLE_ENV",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:346239809653:secret:aura-vote/secrets:PADDLE_ENV::"
        },
        {
          "name": "PORT",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:346239809653:secret:aura-vote/secrets:PORT::"
        }
      ]
    }
  ],
  "requiresAttributes": [
    {
      "name": "com.amazonaws.ecs.capability.logging-driver.awslogs"
    },
    {
      "name": "ecs.capability.execution-role-awslogs"
    },
    {
      "name": "com.amazonaws.ecs.capability.ecr-auth"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.19"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.28"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.21"
    },
    {
      "name": "com.amazonaws.ecs.capability.task-iam-role"
    },
    {
      "name": "ecs.capability.execution-role-ecr-pull"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.18"
    },
    {
      "name": "ecs.capability.task-eni"
    },
    {
      "name": "com.amazonaws.ecs.capability.docker-remote-api.1.29"
    }
  ]
}
