{
  "name": "@upvote-app/server",
  "version": "1.0.0",
  "description": "Express server for upvote app",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register server.ts",
