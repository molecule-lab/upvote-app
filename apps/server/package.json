    "@types/multer": "^1.4.12",
    "@types/pg": "^8.15.4",
    "add": "^2.0.6",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.41.0",
    "express": "^4.18.2",
    "firebase-admin": "^13.4.0",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "pg": "^8.16.0",
    "pg-core@latest": "link:drizzle-orm/pg-core@latest",
